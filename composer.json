{
    "name": "dmt-software/xsd-builder",
    "description": "Create XSD using PHP",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Bas de Mes",
            "email": "bas@dmt-software.nl"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-dom": "*",
        "spiral/attributes": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "DMT\\XsdBuilder\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=10.5",
        "ext-libxml": "*",
        "squizlabs/php_codesniffer": "^3.11"
    },
    "autoload-dev": {
        "psr-4": {
            "DMT\\Test\\XsdBuilder\\": "tests/"
        }
    }
}
